net: "model/CNN/CNN_train.prototxt"

#Parametros de entrenamiento
base_lr: 0.001
momentum: 0.9
weight_decay: 0.004

#Decaimiento de la tasa de aprendizaje
lr_policy: "fixed"

#Parametros de debug
display: 100

#Numero de iteraciones
max_iter: 4000
test_iter: 100
test_interval: 500

#Guardado de pesos
snapshot: 4000
snapshot_prefix: "weights/CNN/model_w"
# solver mode: CPU or GPU
#solver_mode: CPU
